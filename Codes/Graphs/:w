#include <bits/stdc++.h>
using namespace std;

const int N = 1e6 + 7;
vector<int> adj[N], tour;

void dfs(int u, int p) {
    tour.push_back(u);
    for (int v : adj[u]) {
        if (v == p) continue;
        dfs(v, u);
        tour.push_back(u);
    }
}

int main() {
    ios::sync_with_stdio(0);
    cin.tie(0);

    int n;
    cin >> n;

    for (int i = 1; i < n; i++) {
        int u, v;
        cin >> u >> v;
        adj[u].push_back(v);
        adj[v].push_back(u);
    }

    dfs(1, 1);
    for(int u : tour) cout<<i<<' ';
}

